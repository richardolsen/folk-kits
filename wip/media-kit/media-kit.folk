when the kit alphaKit exists & the media kit does not exist {

    set pageList {
        {0 media-kit.folk}
        {2 webupload.folk}
    }

    Wish $this kit called media is installed with pages $pageList
}

when the alpha kit exists & the media kit exists {

    # When installing from the new-virtual-kits folder on your folk system
    
    claim mediacard kit has pages {
        {1 media-kit.folk} 
        {2 webupload.folk}
    }
}
